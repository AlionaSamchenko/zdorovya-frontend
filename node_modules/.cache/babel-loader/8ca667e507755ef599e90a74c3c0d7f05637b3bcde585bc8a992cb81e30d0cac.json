 from 'react';\r\nimport './HomePage.css';\r\n\r\nfunction HomePage() {\r\n  return (\r\n    <div className=\"home-container\">\r\n\r\n      {/* 1. Про нас */}\r\n      <section className=\"home-section about-section\">\r\n        <h2>Про нас</h2>\r\n        <p>\r\n          Ми створили цей сайт для того, щоб допомогти людям зрозуміти важливість слідкувати за своїм здоровим способом життя.\r\n          Тут ви зможете знайти корисні поради, навчитися правильно харчуватися, підтримувати водний баланс, \r\n          зможете підібрати дієту та слідкувати за своєю активністю.\r\n        </p>\r\n      </section>\r\n\r\n      {/* 2. Картинки з текстом ПІД картинкою в окремих блоках */}\r\n      <section className=\"home-section image-cards-section\">\r\n\r\n        <div className=\"image-card\">\r\n          <img src=\"/healthy_food.jpg\" alt=\"Здорове харчування\" />\r\n          <div className=\"card-text\">\r\n            <h3>Здорове харчування</h3>\r\n            <p>Слідкуйте за харчуванням разом з нами. Навчись вибирати правильні продукти харчування.</p>\r\n          </div>\r\n        </div>\r\n\r\n        <div className=\"image-card\">\r\n          <img src=\"/sport_activity.jpg\" alt=\"Фізична активність\" />\r\n          <div className=\"card-text\">\r\n            <h3>Фізична активність</h3>\r\n            <p>Подбайте про свою активність. Активний спосіб життя - це перший крок для досягання цілі.</p>\r\n          </div>\r\n        </div>\r\n\r\n        <div className=\"image-card\">\r\n          <img src=\"/water_balance.jpg\" alt=\"Водний баланс\" />\r\n          <div className=\"card-text\">\r\n            <h3>Водний баланс</h3>\r\n            <p>Слідкуйте за кількістю питної води. Ми допоможемо тобі у цьому!</p>\r\n          </div>\r\n        </div>\r\n\r\n      </section>\r\n\r\n      {/* 3. Заголовок + кнопка */}\r\n      <section className=\"highlight-section\">\r\n        <h2>Зроби перший крок до здорового життя вже сьогодні!</h2>\r\n        <button className=\"action-button\" onClick={() => window.location.href='/calculatorpage'}>\r\n          Почнемо зараз\r\n        </button>\r\n      </section>\r\n\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default HomePage;"],"mappings":"AAAA,MAAO,CAAAA,KAAK,KAAM,OAAO,CACzB,MAAO,gBAAgB,CAAC,OAAAC,GAAA,IAAAC,IAAA,CAAAC,IAAA,IAAAC,KAAA,yBAExB,QAAS,CAAAC,QAAQA,CAAA,CAAG,CAClB,mBACED,KAAA,QAAKE,SAAS,CAAC,gBAAgB,CAAAC,QAAA,eAG7BH,KAAA,YAASE,SAAS,CAAC,4BAA4B,CAAAC,QAAA,eAC7CL,IAAA,OAAAK,QAAA,CAAI,uCAAO,CAAI,CAAC,cAChBL,IAAA,MAAAK,QAAA,CAAG,45CAIH,CAAG,CAAC,EACG,CAAC,cAGVH,KAAA,YAASE,SAAS,CAAC,kCAAkC,CAAAC,QAAA,eAEnDH,KAAA,QAAKE,SAAS,CAAC,YAAY,CAAAC,QAAA,eACzBL,IAAA,QAAKM,GAAG,CAAC,mBAAmB,CAACC,GAAG,CAAC,yGAAoB,CAAE,CAAC,cACxDL,KAAA,QAAKE,SAAS,CAAC,WAAW,CAAAC,QAAA,eACxBL,IAAA,OAAAK,QAAA,CAAI,yGAAkB,CAAI,CAAC,cAC3BL,IAAA,MAAAK,QAAA,CAAG,0cAAsF,CAAG,CAAC,EAC1F,CAAC,EACH,CAAC,cAENH,KAAA,QAAKE,SAAS,CAAC,YAAY,CAAAC,QAAA,eACzBL,IAAA,QAAKM,GAAG,CAAC,qBAAqB,CAACC,GAAG,CAAC,yGAAoB,CAAE,CAAC,cAC1DL,KAAA,QAAKE,SAAS,CAAC,WAAW,CAAAC,QAAA,eACxBL,IAAA,OAAAK,QAAA,CAAI,yGAAkB,CAAI,CAAC,cAC3BL,IAAA,MAAAK,QAAA,CAAG,kcAAwF,CAAG,CAAC,EAC5F,CAAC,EACH,CAAC,cAENH,KAAA,QAAKE,SAAS,CAAC,YAAY,CAAAC,QAAA,eACzBL,IAAA,QAAKM,GAAG,CAAC,oBAAoB,CAACC,GAAG,CAAC,2EAAe,CAAE,CAAC,cACpDL,KAAA,QAAKE,SAAS,CAAC,WAAW,CAAAC,QAAA,eACxBL,IAAA,OAAAK,QAAA,CAAI,2EAAa,CAAI,CAAC,cACtBL,IAAA,MAAAK,QAAA,CAAG,qUAA+D,CAAG,CAAC,EACnE,CAAC,EACH,CAAC,EAEC,CAAC,cAGVH,KAAA,YAASE,SAAS,CAAC,mBAAmB,CAAAC,QAAA,eACpCL,IAAA,OAAAK,QAAA,CAAI,sQAAkD,CAAI,CAAC,cAC3DL,IAAA,WAAQI,SAAS,CAAC,eAAe,CAACI,OAAO,CAAEA,CAAA,GAAMC,MAAM,CAACC,QAAQ,CAACC,IAAI,CAAC,iBAAkB,CAAAN,QAAA,CAAC,2EAEzF,CAAQ,CAAC,EACF,CAAC,EAEP,CAAC,CAEV,CAEA,cAAe,CAAAF,QAAQ","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}