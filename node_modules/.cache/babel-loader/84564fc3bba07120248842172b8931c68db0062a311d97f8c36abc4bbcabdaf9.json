440\\u0445\\u043D\\u044E \\u0447\\u0430\\u0441\\u0442\\u0438\\u043D\\u0443 \\u0442\\u0443\\u043B\\u0443\\u0431\\u0430, \\u0441\\u043A\\u0440\\u0443\\u0447\\u0443\\u044E\\u0447\\u0438 \\u043A\\u043E\\u0440\\u043F\\u0443\\u0441, \\u043F\\u043E\\u0442\\u0456\\u043C \\u043F\\u043E\\u0432\\u0435\\u0440\\u043D\\u0443\\u0442\\u0438\\u0441\\u044F \\u0432\\u043D\\u0438\\u0437.\"}),/*#__PURE__*/_jsx(\"img\",{src:\"/crunches.jpg\",alt:\"\\u0421\\u043A\\u0440\\u0443\\u0447\\u0443\\u0432\\u0430\\u043D\\u043D\\u044F\",className:\"exercise-image\"})]}),/*#__PURE__*/_jsxs(\"div\",{className:\"exercise\",children:[/*#__PURE__*/_jsx(\"h4\",{children:\"\\u0412\\u0435\\u043B\\u043E\\u0441\\u0438\\u043F\\u0435\\u0434\"}),/*#__PURE__*/_jsx(\"p\",{children:\"\\u041B\\u044F\\u0433\\u0442\\u0438 \\u043D\\u0430 \\u0441\\u043F\\u0438\\u043D\\u0443, \\u043D\\u043E\\u0433\\u0438 \\u043F\\u0456\\u0434\\u043D\\u044F\\u0442\\u0438 \\u0456 \\u0456\\u043C\\u0456\\u0442\\u0443\\u0432\\u0430\\u0442\\u0438 \\u0440\\u0443\\u0445\\u0438, \\u043D\\u0456\\u0431\\u0438 \\u0457\\u0434\\u0435\\u0442\\u0435 \\u043D\\u0430 \\u0432\\u0435\\u043B\\u043E\\u0441\\u0438\\u043F\\u0435\\u0434\\u0456, \\u0442\\u043E\\u0440\\u043A\\u0430\\u044E\\u0447\\u0438\\u0441\\u044C \\u043B\\u0456\\u043A\\u0442\\u0435\\u043C \\u043F\\u0440\\u043E\\u0442\\u0438\\u043B\\u0435\\u0436\\u043D\\u043E\\u0433\\u043E \\u043A\\u043E\\u043B\\u0456\\u043D\\u0430.\"}),/*#__PURE__*/_jsx(\"img\",{src:\"/bicycle-crunch.jpg\",alt:\"\\u0412\\u0435\\u043B\\u043E\\u0441\\u0438\\u043F\\u0435\\u0434\",className:\"exercise-image\"})]})]}),/*#__PURE__*/_jsxs(\"section\",{className:\"activity-section\",id:\"full-body-exercises\",children:[/*#__PURE__*/_jsx(\"h2\",{className:\"exercise-title\",children:\"\\u0412\\u043F\\u0440\\u0430\\u0432\\u0438 \\u0434\\u043B\\u044F \\u0432\\u0441\\u044C\\u043E\\u0433\\u043E \\u0442\\u0456\\u043B\\u0430\"}),/*#__PURE__*/_jsxs(\"div\",{className:\"exercise\",children:[/*#__PURE__*/_jsx(\"h4\",{children:\"\\u0411\\u0443\\u0440\\u043F\\u0456\"}),/*#__PURE__*/_jsx(\"p\",{children:\"\\u0417 \\u043F\\u043E\\u043B\\u043E\\u0436\\u0435\\u043D\\u043D\\u044F \\u0441\\u0442\\u043E\\u044F\\u0447\\u0438 \\u043E\\u043F\\u0443\\u0441\\u0442\\u0438\\u0442\\u0438\\u0441\\u044F \\u0432 \\u043F\\u0440\\u0438\\u0441\\u0456\\u0434\\u0430\\u043D\\u043D\\u044F, \\u043F\\u043E\\u043A\\u043B\\u0430\\u0441\\u0442\\u0438 \\u0440\\u0443\\u043A\\u0438 \\u043D\\u0430 \\u043F\\u0456\\u0434\\u043B\\u043E\\u0433\\u0443, \\u0432\\u0438\\u043F\\u0440\\u044F\\u043C\\u0438\\u0442\\u0438 \\u043D\\u043E\\u0433\\u0438 \\u043D\\u0430\\u0437\\u0430\\u0434 \\u0443 \\u043F\\u043E\\u043B\\u043E\\u0436\\u0435\\u043D\\u043D\\u044F \\u043F\\u043B\\u0430\\u043D\\u043A\\u0438, \\u0437\\u0440\\u043E\\u0431\\u0438\\u0442\\u0438 \\u0432\\u0456\\u0434\\u0436\\u0438\\u043C\\u0430\\u043D\\u043D\\u044F, \\u043F\\u043E\\u0432\\u0435\\u0440\\u043D\\u0443\\u0442\\u0438\\u0441\\u044F \\u0432 \\u043F\\u0440\\u0438\\u0441\\u0456\\u0434\\u0430\\u043D\\u043D\\u044F \\u0456 \\u043F\\u0456\\u0434\\u0441\\u0442\\u0440\\u0438\\u0431\\u043D\\u0443\\u0442\\u0438 \\u0432\\u0433\\u043E\\u0440\\u0443.\"}),/*#__PURE__*/_jsx(\"img\",{src:\"/burpee.jpg\",alt:\"\\u0411\\u0443\\u0440\\u043F\\u0456\",className:\"exercise-image\"})]}),/*#__PURE__*/_jsxs(\"div\",{className:\"exercise\",children:[/*#__PURE__*/_jsx(\"h4\",{children:\"\\u0421\\u0442\\u0440\\u0438\\u0431\\u043A\\u0438 \\u0437 \\u0440\\u043E\\u0437\\u0432\\u0435\\u0434\\u0435\\u043D\\u043D\\u044F\\u043C \\u0440\\u0443\\u043A \\u0456 \\u043D\\u0456\\u0433 (Jumping Jacks)\"}),/*#__PURE__*/_jsx(\"p\",{children:\"\\u0421\\u0442\\u0430\\u0442\\u0438 \\u043F\\u0440\\u044F\\u043C\\u043E, \\u0440\\u0443\\u043A\\u0438 \\u043E\\u043F\\u0443\\u0449\\u0435\\u043D\\u0456. \\u0421\\u0442\\u0440\\u0438\\u0431\\u043D\\u0443\\u0442\\u0438, \\u0440\\u043E\\u0437\\u0432\\u043E\\u0434\\u044F\\u0447\\u0438 \\u043D\\u043E\\u0433\\u0438 \\u0432 \\u0441\\u0442\\u043E\\u0440\\u043E\\u043D\\u0438 \\u0456 \\u043F\\u0456\\u0434\\u043D\\u0456\\u043C\\u0430\\u044E\\u0447\\u0438 \\u0440\\u0443\\u043A\\u0438 \\u043D\\u0430\\u0434 \\u0433\\u043E\\u043B\\u043E\\u0432\\u043E\\u044E. \\u041F\\u043E\\u0432\\u0442\\u043E\\u0440\\u0438\\u0442\\u0438.\"}),/*#__PURE__*/_jsx(\"img\",{src:\"/jumping-jacks.jpg\",alt:\"\\u0421\\u0442\\u0440\\u0438\\u0431\\u043A\\u0438 \\u0437 \\u0440\\u043E\\u0437\\u0432\\u0435\\u0434\\u0435\\u043D\\u043D\\u044F\\u043C \\u0440\\u0443\\u043A \\u0456 \\u043D\\u0456\\u0433\",className:\"exercise-image\"})]}),/*#__PURE__*/_jsxs(\"div\",{className:\"exercise\",children:[/*#__PURE__*/_jsx(\"h4\",{children:\"\\u0412\\u0456\\u0434\\u0436\\u0438\\u043C\\u0430\\u043D\\u043D\\u044F\"}),/*#__PURE__*/_jsx(\"p\",{children:\"\\u041E\\u043F\\u0443\\u0441\\u0442\\u0438\\u0442\\u0438\\u0441\\u044F \\u0443 \\u043F\\u043E\\u043B\\u043E\\u0436\\u0435\\u043D\\u043D\\u044F \\u043F\\u043B\\u0430\\u043D\\u043A\\u0438 \\u043D\\u0430 \\u0440\\u0443\\u043A\\u0430\\u0445, \\u0437\\u0456\\u0433\\u043D\\u0443\\u0442\\u0438 \\u0440\\u0443\\u043A\\u0438 \\u0432 \\u043B\\u0456\\u043A\\u0442\\u044F\\u0445, \\u043E\\u043F\\u0443\\u0441\\u043A\\u0430\\u044E\\u0447\\u0438 \\u0442\\u0456\\u043B\\u043E, \\u043F\\u043E\\u0442\\u0456\\u043C \\u0432\\u0438\\u043F\\u0440\\u044F\\u043C\\u0438\\u0442\\u0438 \\u0440\\u0443\\u043A\\u0438.\"}),/*#__PURE__*/_jsx(\"img\",{src:\"/pushupsd.jpg\",alt:\"\\u0412\\u0456\\u0434\\u0436\\u0438\\u043C\\u0430\\u043D\\u043D\\u044F\",className:\"exercise-image\"})]})]}),/*#__PURE__*/_jsx(\"button\",{className:\"back-button\",onClick:()=>navigate('/'),children:\"\\u041D\\u0430 \\u0433\\u043E\\u043B\\u043E\\u0432\\u043D\\u0443\"})]});}export default ActivityPage;","map":{"version":3,"names":["React","useNavigate","jsx","_jsx","jsxs","_jsxs","ActivityPage","navigate","className","children","href","id","src","alt","onClick"],"sources":["C:/Users/think/my-app/src/pages/ActivityPage.js"],"sourcesContent":["import React from 'react';\r\nimport { useNavigate } from 'react-router-dom';\r\nimport './ActivityPage.css';\r\n\r\nfunction ActivityPage() {\r\n  const navigate = useNavigate();\r\n\r\n  return (\r\n    <div className=\"activity-page\">\r\n      <h1 className=\"page-title\">Активність</h1>\r\n\r\n      {/* Навигация по разделам */}\r\n      <nav className=\"activity-nav\">\r\n        <a href=\"#what-is-activity\">Що таке активність?</a>\r\n        <a href=\"#arms-exercises\">Вправи для рук</a>\r\n        <a href=\"#legs-exercises\">Вправи для ніг</a>\r\n        <a href=\"#abs-exercises\">Вправи для пресу</a>\r\n        <a href=\"#full-body-exercises\">Вправи для всього тіла</a>\r\n      </nav>\r\n\r\n      {/* Что такое активность */}\r\n      <section className=\"activity-section\" id=\"what-is-activity\">\r\n        <h2>Що таке активність та навіщо вона нам потрібна?</h2>\r\n  <p>\r\n    Рухова активність — це будь-які форми руху, що потребують енергії, наприклад ходьба, фізична робота, фізичні вправи, заняття спортом тощо.\r\n  </p>\r\n  <p>\r\n    Рухова активність потрібна нашому життю дуже сильно. Саме вона слугує підтримкою для нашого здоров'я, а саме:\r\n  </p>\r\n  <ul>\r\n    <li>покращує роботу сердця, знижує ризик серцевих захворювань та інсульту;</li>\r\n    <li>дозволяє спалювати калорії, що надає можливість підтримувати вагу у нормі;</li>\r\n    <li>допомогає збільшити м'язову масу, зміцнити кістки;</li>\r\n    <li>прискорює обмін речовин та роботу дихальної системи;</li>\r\n    <li>стимулює вироблення ендорфіну, що покращує настрій та знижує рівень стресу;</li>\r\n    <li>збільшує тривалість життя.</li>\r\n  </ul>\r\n  <p>\r\n    Не забувайте приділяти увагу фізичній активності вашого організму. Навіть сама маленька активність краще, ніж повна її відсутність у вашому житті.\r\n  </p>\r\n      </section>\r\n\r\n      {/* Вправи для рук */}\r\n      <section className=\"activity-section\" id=\"arms-exercises\">\r\n        <h2 className=\"exercise-title\" >Вправи для рук</h2>\r\n\r\n        <div className=\"exercise\">\r\n          <h4>Віджимання від підлоги</h4>\r\n          <p>\r\n            Лягти на живіт, руки розташувати на ширині плечей. Зігнути руки в ліктях і опустити корпус до підлоги, потім випрямити руки, піднімаючи корпус.\r\n          </p>\r\n          <img\r\n            src=\"/pushups.jpg\"\r\n            alt=\"Віджимання від підлоги\"\r\n            className=\"exercise-image\"\r\n          />\r\n        </div>\r\n\r\n        <div className=\"exercise\">\r\n          <h4>Підйом гантелей на біцепс</h4>\r\n          <p>\r\n            Взяти гантелі в руки, стоячи прямо. Повільно згинати руки в ліктях, піднімаючи гантелі до плечей, потім повільно опускати.\r\n          </p>\r\n          <img\r\n            src=\"/bicep-curls.jpg\"\r\n            alt=\"Підйом гантелей на біцепс\"\r\n            className=\"exercise-image\"\r\n          />\r\n        </div>\r\n\r\n        <div className=\"exercise\">\r\n          <h4>Жим гантелей лежачи</h4>\r\n          <p>\r\n            Лягти на лавку або підлогу, тримати гантелі на рівні грудей. Випрямити руки вгору, потім опустити.\r\n          </p>\r\n          <img\r\n            src=\"/dumbbell-press.jpg\"\r\n            alt=\"Жим гантелей лежачи\"\r\n            className=\"exercise-image\"\r\n          />\r\n        </div>\r\n      </section>\r\n\r\n      {/* Вправи для ніг */}\r\n      <section className=\"activity-section\" id=\"legs-exercises\">\r\n        <h2 className=\"exercise-title\" >Вправи для ніг</h2>\r\n\r\n        <div className=\"exercise\">\r\n          <h4>Присідання</h4>\r\n          <p>\r\n            Стати прямо, ноги на ширині плечей. Згинати коліна і опускати таз, наче сідаєте на стілець, потім повертатися у початкове положення.\r\n          </p>\r\n          <img\r\n            src=\"/squats.jpg\"\r\n            alt=\"Присідання\"\r\n            className=\"exercise-image\"\r\n          />\r\n        </div>\r\n\r\n        <div className=\"exercise\">\r\n          <h4>Випади вперед</h4>\r\n          <p>\r\n            Зробити крок вперед однією ногою і опустити тіло вниз, згинаючи коліна. Повернутися у початкове положення і повторити на іншу ногу.\r\n          </p>\r\n          <img\r\n            src=\"/lunges.jpg\"\r\n            alt=\"Випади вперед\"\r\n            className=\"exercise-image\"\r\n          />\r\n        </div>\r\n\r\n        <div className=\"exercise\">\r\n          <h4>Місток</h4>\r\n          <p>\r\n            Лягти на спину, ноги зігнуті в колінах, стопи на підлозі. Підняти таз вгору, стискаючи сідниці, потім повільно опустити.\r\n          </p>\r\n          <img\r\n            src=\"/bridge.jpg\"\r\n            alt=\"Місток\"\r\n            className=\"exercise-image\"\r\n          />\r\n        </div>\r\n      </section>\r\n\r\n      {/* Вправи для пресу */}\r\n      <section className=\"activity-section\" id=\"abs-exercises\">\r\n        <h2 className=\"exercise-title\" >Вправи для пресу</h2>\r\n\r\n        <div className=\"exercise\">\r\n          <h4>Планка</h4>\r\n          <p>\r\n            Прийняти положення упору на передпліччях і носках, тримати тіло рівно, не провисати і не піднімати таз.\r\n          </p>\r\n          <img\r\n            src=\"/plank.jpg\"\r\n            alt=\"Планка\"\r\n            className=\"exercise-image\"\r\n          />\r\n        </div>\r\n\r\n        <div className=\"exercise\">\r\n          <h4>Скручування</h4>\r\n          <p>\r\n            Лягти на спину, ноги зігнуті, руки за головою. Підняти верхню частину тулуба, скручуючи корпус, потім повернутися вниз.\r\n          </p>\r\n          <img\r\n            src=\"/crunches.jpg\"\r\n            alt=\"Скручування\"\r\n            className=\"exercise-image\"\r\n          />\r\n        </div>\r\n\r\n        <div className=\"exercise\">\r\n          <h4>Велосипед</h4>\r\n          <p>\r\n            Лягти на спину, ноги підняти і імітувати рухи, ніби їдете на велосипеді, торкаючись ліктем протилежного коліна.\r\n          </p>\r\n          <img\r\n            src=\"/bicycle-crunch.jpg\"\r\n            alt=\"Велосипед\"\r\n            className=\"exercise-image\"\r\n          />\r\n        </div>\r\n      </section>\r\n\r\n      {/* Вправи для всього тіла */}\r\n      <section className=\"activity-section\" id=\"full-body-exercises\">\r\n        <h2 className=\"exercise-title\" >Вправи для всього тіла</h2>\r\n\r\n        <div className=\"exercise\">\r\n          <h4>Бурпі</h4>\r\n          <p>\r\n            З положення стоячи опуститися в присідання, покласти руки на підлогу, випрямити ноги назад у положення планки, зробити віджимання, повернутися в присідання і підстрибнути вгору.\r\n          </p>\r\n          <img\r\n            src=\"/burpee.jpg\"\r\n            alt=\"Бурпі\"\r\n            className=\"exercise-image\"\r\n          />\r\n        </div>\r\n\r\n        <div className=\"exercise\">\r\n          <h4>Стрибки з розведенням рук і ніг (Jumping Jacks)</h4>\r\n          <p>\r\n            Стати прямо, руки опущені. Стрибнути, розводячи ноги в сторони і піднімаючи руки над головою. Повторити.\r\n          </p>\r\n          <img\r\n            src=\"/jumping-jacks.jpg\"\r\n            alt=\"Стрибки з розведенням рук і ніг\"\r\n            className=\"exercise-image\"\r\n          />\r\n        </div>\r\n\r\n        <div className=\"exercise\">\r\n          <h4>Віджимання</h4>\r\n          <p>\r\n            Опуститися у положення планки на руках, зігнути руки в ліктях, опускаючи тіло, потім випрямити руки.\r\n          </p>\r\n          <img\r\n            src=\"/pushupsd.jpg\"\r\n            alt=\"Віджимання\"\r\n            className=\"exercise-image\"\r\n          />\r\n        </div>\r\n      </section>\r\n\r\n      <button className=\"back-button\" onClick={() => navigate('/')}>На головну</button>\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default ActivityPage;\r\n\r\n"],"mappings":"AAAA,MAAO,CAAAA,KAAK,KAAM,OAAO,CACzB,OAASC,WAAW,KAAQ,kBAAkB,CAC9C,MAAO,oBAAoB,CAAC,OAAAC,GAAA,IAAAC,IAAA,CAAAC,IAAA,IAAAC,KAAA,yBAE5B,QA